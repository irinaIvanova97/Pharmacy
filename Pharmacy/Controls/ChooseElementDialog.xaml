<b_controls:BaseDialog x:Class="Pharmacy.Controls.ChooseElementDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pharmacy.Controls"
        xmlns:b_controls="clr-namespace:Pharmacy.Controls"
        mc:Ignorable="d"
        Title="Избиране на" Height="370" Width="530" WindowStartupLocation="CenterOwner">
    <Grid>
        <Grid x:Name="ui_gridListView" Margin="10,10,10,70" />
        <Button x:Name="ui_btnChoose" Content="Избери" HorizontalAlignment="Right" Margin="0,0,90,10" VerticalAlignment="Bottom" Width="75"/>
        <Button x:Name="ui_btnCancel" Content="Отказ" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75"/>
    </Grid>
</b_controls:BaseDialog>
